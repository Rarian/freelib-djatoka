<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [ %C{5} , %M , %L ] %m %n</pattern>
        </encoder>
    </appender>

    <logger name="info.freelibrary.djatoka.view.IdentifierResolver" level="INFO"/>

    <root level="debug">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
